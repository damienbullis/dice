{
  "name": "@damienbullis/dice",
  "version": "0.1.0",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/damienbullis/dice"
  },
  "description": "A dice rolling package for rolling polyhedral & funky dice",
  "author": {
    "name": "Damien Bullis",
    "url": "https://github.com/damienbullis"
  },
  "keywords": [
    "dice",
    "funky dice",
    "funky",
    "roll",
    "d20"
  ],
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "private": false,
  "scripts": {
    "build": "tsup src/index.ts --format cjs,esm --dts",
    "release": "changeset publish --access=public",
    "dev": "tsup src/index.ts --watch --format cjs,esm --dts",
    "lint": "tsc"
  },
  "devDependencies": {
    "@changesets/cli": "^2.26.0",
    "tsup": "^6.5.0",
    "typescript": "^4.9.4"
  }
}
