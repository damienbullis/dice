{
  "name": "@damienbullis/dice",
  "version": "0.2.1",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/damienbullis/dice"
  },
  "description": "A dice rolling package for rolling polyhedral & funky dice",
  "author": {
    "name": "Damien Bullis",
    "url": "https://github.com/damienbullis"
  },
  "packageManager": "pnpm@8.1.0",
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "keywords": [
    "dice",
    "funky dice",
    "funky",
    "roll",
    "d20"
  ],
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "private": false,
  "scripts": {
    "build": "tsup src/index.ts --format cjs,esm --dts",
    "release": "changeset publish",
    "dev": "tsup src/index.ts --watch --format cjs,esm --dts",
    "test": "vitest",
    "coverage": "vitest run --coverage",
    "lint": "tsc"
  },
  "devDependencies": {
    "@changesets/cli": "^2.26.0",
    "@types/node": "^18.15.13",
    "@vitest/coverage-c8": "^0.30.1",
    "tsup": "^6.5.0",
    "typescript": "^4.9.4",
    "vite": "^4.3.1",
    "vitest": "^0.30.1"
  },
  "peerDependencies": {
    "@types/node": "^18.15.13"
  }
}
